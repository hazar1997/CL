pipeline {
    agent any
    stages {
        
        stage('pull') { 
            steps {
              script {
                   checkout([$class: 'GitSCM',branches: [[name: '*/hazar']],
                      userRemoteConfigs: [[
                        credentiasId: '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',
                        url: 'https://github.com/hazar1997/CII.git']]]) 
               
            }
        }

     }
     stage('Build'){
        steps {
         script {
           sh "ansible-playbook ansible/build.yml -i ansible/inventory/host.yml"
           }
             }
           } 
}
} 
